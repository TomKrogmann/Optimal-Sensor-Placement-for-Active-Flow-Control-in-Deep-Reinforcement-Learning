        //writeControl    timeStep;
        //writeControl    writeTime;
        //executeControl  timeStep;
        //executeInterval 2;
        writeControl    timeStep;
        writeInterval   250;
        timeStart       375;
        operation       sum;
        regionType      faceZone;
        name            faceZone_c;
        weightField     none;
        writeFields     yes;
        surfaceFormat   raw;

    }

